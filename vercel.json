{
  "builds": [
    {
      "src": "apps/admin-app/package.json",
      "use": "@vercel/node"
    }
  ],
  "installCommand": "corepack enable && corepack prepare pnpm@8.9.0 --activate && pnpm install --filter admin-app...",
  "buildCommand": "pnpm run build",
  "outputDirectory": "apps/admin-app/.next",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
